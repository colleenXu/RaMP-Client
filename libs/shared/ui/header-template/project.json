{
  "projectType": "library",
  "root": "libs/shared/ui/header-template",
  "sourceRoot": "libs/shared/ui/header-template/src",
  "prefix": "ramp",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/ui/header-template"],
      "options": {
        "jestConfig": "libs/shared/ui/header-template/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "stylePreprocessorOptions": {
        "includePaths": ["libs/styles/src/styles/"]
      },
      "options": {
        "lintFilePatterns": [
          "libs/shared/ui/header-template/src/**/*.ts",
          "libs/shared/ui/header-template/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
